	"repository": {
		"type": "git",
		"url": "https://github.com/xk0der/vscode-postman-log-syntax.git"
	},
	"keywords": [
		"Postman",
		"Collection",
        "Syntax",
        "API"
	],
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "postman_collection",
                "aliases": [
                    "Postman Collection",
                    "postmancollection"
                ],
                "extensions": [
                    ".postman_collection.json"
                ],
                "configuration": "./postman_collection-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "postman_collection",
                "scopeName": "source.postman_collection.json",
                "path": "./syntaxes/collection.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.javascript": "source.js"
                }
            }
        ]
    }
