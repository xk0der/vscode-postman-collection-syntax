{
    "name": "vsc-postman-collection-syntax",
    "displayName": "Postman Collection Syntax Highlighter",
    "description": "",
    "version": "0.0.1",
    "icon": "Postman-Collection.png",
	"license": "MIT",
	"publisher": "xk0der",
	"author": {
		"name": "xk0der",
		"email": "amit@xkoder.com",
